{
    "extends": "recommended",
    "rules": {
        "CompileStatic": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "MethodParameterTypeRequired": {
            "enabled": false
        },
        "MethodReturnTypeRequired": {
            "enabled": false
        },
        "NoDef": {
            "enabled": false
        },
        "formatting.Indentation": {
            "spacesPerIndentLevel": 4,
            "severity": "info"
        }
    }
}